{
  "name": "buildit-network",
  "version": "0.1.0",
  "type": "module",
  "description": "A privacy-first organizing platform built on Nostr protocol for activist groups, co-ops, and community organizers.",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
